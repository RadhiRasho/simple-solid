{
  "compilerOptions": {
			"target": "ESNext",
			"module": "NodeNext",
			"moduleResolution": "NodeNext",
			"resolveJsonModule": true,
			"allowSyntheticDefaultImports": true,
			"esModuleInterop": true,
			"jsx": "preserve",
			"jsxImportSource": "solid-js",
			"allowJs": true,
			"noEmit": true,
			"strict": true,
			"types": ["vinxi/types/client"],
			"isolatedModules": true,
			"baseUrl": "./",
			"paths": {
				"~/*": ["./src/*"]
			}
		}
}
